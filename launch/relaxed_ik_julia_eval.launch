<?xml version="1.0"?>
<launch>
  <arg name="info_file_name"/>
  <arg name="out_file_root" default="./noise"/>

  <param name="ready" value="false"/>
  <rosparam command='load' file='$(find lively_ik)/src/RelaxedIK/Config/info_files/$(arg info_file_name)'/>

  <include file="$(find lively_ik)/launch/load_info_file.launch">
    <arg name="info_file_name" value="$(arg info_file_name)"/>
  </include>

  <node pkg="lively_ik" type="compare_relaxed_ik_node.jl" name="lively_ik_node" output="screen"/>
  <!-- <node pkg="lively_ik_orig" type="relaxed_ik_node.jl" name="lively_ik_node" output="screen"/> -->
  <node pkg="lively_ik" type="eval_lively_ik.py" name="eval_lively_ik" output="screen"
        args="--file_root $(arg out_file_root)"/>

  <!-- <node pkg="lively_ik" type="pub_to_lively_ik.py" name="lively_ik_eval_generator" output="screen"/> -->

</launch>
